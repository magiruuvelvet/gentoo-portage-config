+static void
+focaltech_moc_identify_wait_finger_cb (FpiDeviceFocaltechMoc *self,
+                                       uint8_t               *buffer_in,
+                                       gsize                  length_in,
+                                       GError                *error)
+{
+  FpCmd *fp_cmd = NULL;
+  uint8_t *finger_status = NULL;
+
+  if (error)
+    {
+      fpi_ssm_mark_failed (self->task_ssm, error);
+      return;
