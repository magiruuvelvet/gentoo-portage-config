+      struct EnrolledInfoItem *item = &data->enrolled_info->items[i];
+
+      if (memcmp (item->uid, uid, FOCALTECH_MOC_UID_PREFIX_LENGTH) == 0)
+        {
+          data->enrolled_info->actived[i] = 1;
+          *pitem = item;
+          *index = i;
+          ret = 0;
+          break;
+        }
+    }
+
+  return ret;
+}
